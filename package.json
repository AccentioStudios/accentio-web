{
  "name": "accentio-web",
  "version": "1.0.1",
  "description": "Elevando el nivel y la experiencia de la web",
  "main": "index.js",
  "engines": {
    "node": "15.x",
    "npm": "7.x"
  },
  "scripts": {
    "heroku-prebuild": "npm install --dev",
    "start": "node ./index.js",
    "start:prod": "cross-env NODE_ENV=production npm start",
    "tlsof": "cross-env-shell NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 node ./index.js",
    "webpack:dev": "webpack --mode development --watch --progress ",
    "webpack:prod": "webpack --mode production --progress",
    "build": "webpack:prod",
    "dev": "concurrently \"npm run webpack:dev\" \"npm run tlsof\"",
    "prod": "concurrently \"npm run webpack:prod\" \"npm run start:prod\"",
    "test": "jest"
  },
  "keywords": [
    "webpage",
    "accentio",
    "web-development",
    "app-development"
  ],
  "author": "Accentio Studios",
  "license": "ISC",
  "dependencies": {
    "animate.css": "^4.1.1",
    "axios": "^0.21.1",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "jade": "^1.11.0",
    "joi": "^17.3.0",
    "lodash": "^4.17.20",
    "nodemon": "^2.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.13",
    "@webpack-cli/init": "^1.1.1",
    "autoprefixer": "^10.2.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "console-log-level": "^1.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.4",
    "postcss-loader": "^5.0.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.6",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
