{
  "name": "accentio-web",
  "version": "1.0.3",
  "description": "Elevando el nivel y la experiencia de la web",
  "main": "index.js",
  "engines": {
    "node": "15.x",
    "npm": "7.x"
  },
  "scripts": {
    "start": "node ./index.js",
    "startprod": "cross-env NODE_ENV=production npm start",
    "tlsof": "cross-env-shell NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 node ./index.js",
    "webpackdev": "webpack --mode development --watch --progress ",
    "webpackprod": "webpack --mode production --progress",
    "build": "webpack:prod",
    "dev": "concurrently \"npm run webpackdev\" \"npm run tlsof\"",
    "prod": "concurrently \"npm run webpackprod\" \"npm run startprod\"",
    "test": "jest"
  },
  "keywords": [
    "webpage",
    "accentio",
    "web-development",
    "app-development"
  ],
  "author": "Accentio Studios",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.13",
    "@webpack-cli/init": "^1.1.1",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.2.4",
    "axios": "^0.21.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "console-log-level": "^1.4.1",
    "copy-webpack-plugin": "^7.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "ejs": "^3.1.5",
    "ejs-loader": "^0.5.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "helmet": "^4.3.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.0.0",
    "jade": "^1.9.2",
    "jest": "^26.6.3",
    "joi": "^17.3.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^5.0.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.6",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
