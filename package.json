{
  "name": "accentio-web",
  "version": "1.2.1",
  "description": "Elevando el nivel y la experiencia de la web",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "serve": "cross-env-shell APP=%npm_package_name% \"echo Starting server (Hot Reload): %npm_package_name%... && nodemon ./index.js",
    "debug": "cross-env-shell DEBUG=%npm_package_name%:* NODE_ENV=development npm run serve",
    "dev": "cross-env NODE_ENV=development npm run serve",
    "prod": "cross-env NODE_ENV=production npm run start",
    "prod-serve": "cross-env NODE_ENV=production npm run serve",
    "test": "jest",
    "build": "./node_modules/.bin/webpack --mode production"
  },
  "keywords": [
    "webpage",
    "accentio",
    "web-development",
    "app-development"
  ],
  "author": "Accentio Studios",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "joi": "^17.3.0",
    "nodemon": "^2.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.4"
  },
  "devDependencies": {
    "@webpack-cli/init": "^1.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.12.2",
    "webpack-cli": "^4.3.1"
  }
}
